{
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: g++.exe build active file",
			"command": "C:\\msys64\\mingw64\\bin\\g++.exe",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"-I",
				"${workspaceFolder}/include",
				"${workspaceFolder}/src/main.cpp",
				"${workspaceFolder}/src/Pallet.cpp",
				"${workspaceFolder}/src/Shelf.cpp",
				"${workspaceFolder}/src/Employee.cpp",
				"${workspaceFolder}/src/Task.cpp",
				"${workspaceFolder}/src/Warehouse.cpp",
				"-o",
				"${fileDirname}\\${fileBasenameNoExtension}.exe"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": ["$gcc"],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "Task generated by Debugger."
		}
	],
	"version": "2.0.0"
}
